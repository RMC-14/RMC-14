    xmlns:cc="clr-namespace:Content.Client._RMC14.Construction"
    xmlns:userInterface="clr-namespace:Content.Client._RMC14.UserInterface"
    Title="Construction"
    MinSize="300 300">
    <ScrollContainer HScrollEnabled="False" HorizontalExpand="True" VerticalExpand="True">
        <BoxContainer Orientation="Vertical">
            <RichTextLabel Name="MaterialLeftLabel" Access="Public" />
