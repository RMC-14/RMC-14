<controls:JoinXenoWindow
    xmlns="https://spacestation14.io"
    xmlns:controls="clr-namespace:Content.Client._RMC14.Lobby"
    Title="{Loc rmc-lobby-join-xeno}"
    MinSize="400 125">
    <BoxContainer Orientation="Vertical" Margin="5">
        <Label Name="Label" Access="Public" />
        <BoxContainer Name="Buttons" Access="Public" Orientation="Horizontal" Margin="0 10 0 0">
            <Button Name="LarvaButton" Access="Public" Text="{Loc rmc-lobby-join-as-larva}" StyleClasses="OpenBoth" HorizontalExpand="True" MinHeight="35" />
        </BoxContainer>
    </BoxContainer>
</controls:JoinXenoWindow>
