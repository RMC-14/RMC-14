<controls:XenoDevolveWindow
    xmlns="https://spacestation14.io"
    xmlns:controls="clr-namespace:Content.Client._RMC14.Xenonids.Evolution"
    xmlns:userInterface="clr-namespace:Content.Client._RMC14.UserInterface"
    Title="{Loc rmc-xeno-evolution-devolve-title}"
    MinSize="300 300">
    <ScrollContainer HScrollEnabled="False" HorizontalExpand="True" VerticalExpand="True">
        <BoxContainer Orientation="Vertical">
            <BoxContainer Orientation="Vertical" Name="DevolutionsContainer" Access="Public" />
            <userInterface:BlueHorizontalSeparator Name="Separator" Access="Public" />
            <Label Name="DestrainLabel" Access="Public" Text="Reset Strain:" />
            <BoxContainer Orientation="Vertical" Name="DestrainContainer" Access="Public" />
        </BoxContainer>
    </ScrollContainer>
</controls:XenoDevolveWindow>
