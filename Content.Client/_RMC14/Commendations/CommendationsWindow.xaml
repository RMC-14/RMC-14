<controls:CommendationsWindow
    xmlns="https://spacestation14.io"
    xmlns:controls="clr-namespace:Content.Client._RMC14.Commendations"
    xmlns:graphics="clr-namespace:Robust.Client.Graphics;assembly=Robust.Client"
    Title="Medals" MinSize="700 450">
    <PanelContainer>
        <PanelContainer.PanelOverride>
            <graphics:StyleBoxFlat 
                BorderColor="#3A3A3A" 
                BorderThickness="1" 
                BackgroundColor="#141414" />
        </PanelContainer.PanelOverride>
        <BoxContainer Orientation="Vertical" Margin="8" SeparationOverride="6">
            <ScrollContainer HScrollEnabled="False" VScrollEnabled="True" VerticalExpand="True" HorizontalExpand="True">
                <BoxContainer Name="Commendations" Access="Public" Orientation="Vertical" HorizontalExpand="True" SeparationOverride="6" />
            </ScrollContainer>
        </BoxContainer>
    </PanelContainer>
</controls:CommendationsWindow>
